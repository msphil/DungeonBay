<tr>
  <td class="character">
    <span class="name"><%= character.name %></span>
    <span class="description"><%= character.description %></span>
    <%= character.id %>
    <span class="campaign">< %= character.campaign.name %></span>
    <span class="timestamp">
      Posted <%= time_ago_in_words(character.created_at) %> ago.
    </span>
  </td>
</tr>
